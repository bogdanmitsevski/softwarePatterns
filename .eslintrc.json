{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base/legacy"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-var": "error", 
        "no-console": "off",
        "new-cap": "off",
        "no-param-reassign": "off",
        "class-methods-use-this": "off",
        "max-classes-per-file": "off",
        "no-use-before-define": "off",
        "no-unused-vars": "off",
        "no-plusplus": "off",
        "consistent-return": "off"
    }
}
